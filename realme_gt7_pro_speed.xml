<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="github"
          fetch="https://github.com/realme-kernel-opensource/"
          revision="master" />  <!-- 修改默认分支为main -->

  <default remote="github"
           sync-j="8" />
  
  <!-- 内核主仓库 -->
  <project path="kernel" name="realme_GT7pro-Speed-AndroidV-kernel-source" revision="master"/> <!-- 明确指定分支 -->

  <!-- Vendor配置 -->
  <project path="vendor" name="realme_GT7pro-Speed-AndroidV-vendor-source" revision="master"/> <!-- 根据实际分支修改 -->

  <!-- 公共组件 -->
  <project path="common" name="realme_GT7pro-Speed-AndroidV-common-source" revision="master"/> <!-- 根据实际分支修改 -->
  
  <!-- 预编译工具链 -->
  <project path="prebuilts/build-tools" name="PixelExperience/prebuilts_build-tools" clone-depth="1" revision="fourteen"/> <!-- 使用PixelExperience仓库的fourteen分支 -->
</manifest>