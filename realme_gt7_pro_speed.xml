<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- 定义远程仓库源 -->
  <!-- 修改远程仓库配置 -->
  <remote name="realme"
          fetch="https://github.com/realme-kernel-opensource/"
          revision="main" />  <!-- 修改默认分支为main -->
  
  <default remote="realme"
           sync-j="8"
           revision="main" />  <!-- 修改默认分支为main -->
  
  <!-- 修正项目分支 -->
  <project path="kernel/msm-6.6" 
           name="realme-kernel-opensource/realme_GT7pro-Speed-AndroidV-kernel-source"  
           revision="main" />
  
  <project path="vendor/realme" 
           name="realme-kernel-opensource/realme_GT7pro-Speed-AndroidV-vendor-source"
           revision="main" />
  
  <project path="common" 
           name="realme-kernel-opensource/realme_GT7pro-Speed-AndroidV-common-source"
           revision="main" />

  <!-- 预编译工具链 -->
  <project path="prebuilts/build-tools" 
           name="PixelExperience/prebuilts_build-tools"
           remote="github"
           revision="fourteen" />
  
  <!-- 新增远程仓库定义 -->
  <remote name="github"
          fetch="https://github.com/"
          revision="main" />
</manifest>